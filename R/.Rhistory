wpAddByTitle <- function(con,title,newtitle=NULL,...) {
  pid <- dbGetQuery(conwp, paste("select * from ",tname("posts")," where post_title=",shQuote(title)))
  if (nrow(pid)==0) {  
    ## let's create it
    pid <- NA
  } else {
    pid <- pid$ID[1]
  }
  if (!is.null(newtitle)) {
    title <- newtitle
  }
  ## add/edit page
  pid <- wpAdd(conwp,post_title=title,...,postid=pid)
  pid
}

